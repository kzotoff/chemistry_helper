<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet id="document" version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"><xsl:output method="html" encoding="utf-8" indent="yes" /><xsl:template match="/all-news-list">	<div data-module="news">		<h3>Список новостей</h3>		<div class="admin-buttons">			<div class="btn btn-default" id="button_add">Добавить</div>		</div>		<table class="tablesorter table table-bordered unified-table">			<thead>				<tr>					<th>Время</th>					<th>Заголовок</th>					<th>Ссылка</th>					<th>Страница</th>					<th>Потоки</th>					<th>Действия</th>				</tr>			</thead>			<tbody>				<xsl:apply-templates select="news" />			</tbody>		</table>	</div></xsl:template><xsl:template match="news">	<tr data-row-id="{id}" data-page="{page}">		<td class="news_stamp"><xsl:value-of select="stamp" /></td>		<td class="news_caption"><xsl:value-of select="caption" /></td>		<td class="news_link"><xsl:value-of select="link" /></td>		<td class="news_page"><xsl:value-of select="page" /></td>		<td class="news_streams"><xsl:value-of select="streams" /></td>		<td class="actionboard">			<span class="row-inline-button" data-action="edit-row"><img src="modules/news/images/pencil.gif" alt="" /></span>			<span class="row-inline-button" data-action="edit-page"><img src="modules/news/images/right_green.gif" alt="" /></span>			<span class="row-inline-button" data-action="delete-row"><img src="modules/news/images/redcross.gif" alt="" /></span>		</td>	</tr></xsl:template></xsl:stylesheet>