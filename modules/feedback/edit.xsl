<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet id="document" version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"><xsl:output method="html" encoding="utf-8" indent="yes" /><xsl:template match="/root">	<form action="./?module=feedback&amp;action=manage&amp;" method="post" data-form-action="template_edit_form">		<input type="hidden" name="module" value="feedback" />		<input type="hidden" name="action" value="update_template" />		<input type="hidden" name="filename" value="{filename}" />		<textarea class="form-control" id="module_admin_edit_content" name="filecontent" rows="12"><xsl:value-of select="content" /></textarea>		<div class="edit-dialog-buttons">			<input type="submit" value="Сохранить" class="btn btn-primary" data-action="feedback-edit-submit" />			<input type="button" value="Отмена" class="btn btn-primary" data-action="feedback-edit-cancel" />		</div>	</form></xsl:template></xsl:stylesheet>